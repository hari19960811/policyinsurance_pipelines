{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "inspolicysystem"
		},
		"ADLS_Landing_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ADLS_Landing'"
		},
		"AzureDatabricks_LS_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'AzureDatabricks_LS'"
		},
		"Policy_SQL_Server_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Policy_SQL_Server'"
		},
		"ADLS_Landing_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://policysysteminsurance.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "inspolicysystem_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ADLS_Landing_accountKey": {
						"value": "[parameters('ADLS_Landing_accountKey')]"
					},
					"AzureDatabricks_LS_accessToken": {
						"value": "[parameters('AzureDatabricks_LS_accessToken')]"
					},
					"Policy_SQL_Server_connectionString": {
						"value": "[parameters('Policy_SQL_Server_connectionString')]"
					},
					"ADLS_Landing_properties_typeProperties_url": {
						"value": "[parameters('ADLS_Landing_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}